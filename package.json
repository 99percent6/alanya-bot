{
  "name": "alanya-bot",
  "version": "1.0.0",
  "description": "Alanya information telegram bot",
  "private": true,
  "main": "dist/index.js",
  "engines": {
    "node": ">=14.15.0"
  },
  "scripts": {
    "dev": "nodemon",
    "dev:inspect": "nodemon --exec \"node --inspect -r ts-node/register src\"",
    "build": "rm -rf dist && yarn run -s build:code && mkdir dist/assets && cp src/assets/Turkey-residence-permit.pdf dist/assets",
    "build:code": "tsc --build",
    "lint": "eslint src/. --ext .ts",
    "start": "cross-env NODE_ENV=production node $node_options_ext dist"
  },
  "author": "Vladimir Zeynalov",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "cross-env": "^3.1.4",
    "express": "^4.17.1",
    "express-healthcheck": "^0.1.0",
    "express-validation": "^3.0.2",
    "form-data": "^4.0.0",
    "http-status-codes": "^2.2.0",
    "ioredis": "^4.28.0",
    "mongodb": "^4.11.0",
    "ts-node": "^8.9.1",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/config": "^0.0.38",
    "@types/express": "^4.17.11",
    "@types/ioredis": "^4.28.1",
    "@types/node": "~14.18.13",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-preset-es2020": "^1.0.2",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.4.1",
    "typescript": "^4.2.4"
  }
}
